[run]
branch = True
omit =
    */examples/*
    */__main__.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    if self\.debug
    raise AssertionError
    raise NotImplementedError
    if 0:
    if __name__ == .__main__.:
    pass

ignore_errors = True
sort = Cover

[html]
directory = coverage_html_report
